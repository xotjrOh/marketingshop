<body class="app position-relative vsc-initialized" style="height: 100%; zoom: 1;" data-aos-easing="ease"
      data-aos-duration="400" data-aos-delay="0">
<div class="zeynep-overlay"></div>

<div class="container-fluid">
    <div class="row">
        <!-- Main -->
        <main class="py-3 px-3 px-xl-5 w-100">
            <div class="d-none" data-searchable=""></div>
            <div class="row mb-3">
                <div class="col-12">
                    <ul class="nav nav-pills d-flex flex-nowrap overflow-auto pb-2 pb-md-0">
                        <li class="mr-2 pill-item"><a href="/customer/subscriptions" class="btn btn-outline-primary">전체</a></li>
                        <li class="mr-2 pill-item"><a class="btn btn-outline-warning"
                                                      href="/customer/subscriptions/active/1">활성화</a></li>
                        <li class="mr-2 pill-item"><a class="btn btn-outline-success"
                                                      href="/customer/subscriptions/completed/1">완료</a></li>
                        <li class="mr-4 pill-item"><a class="btn btn-outline-secondary"
                                                      href="/customer/subscriptions/canceled/1">취소</a></li>
                        <li><a class="btn btn-outline-info" href="#" data-toggle="modal" data-target="#subsConditionModal">
                            <i class="fas fa-info-circle fa-sm fa-fw"></i>필독</a></li>
                    </ul>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="card border-0">
                        <div class="card-body p-0">
                            <div class="table-responsive">
                                <table class="table modern m-0">
                                    <thead>
                                    <tr>
                                        <th>주문번호</th>
                                        <th>유저이름</th>
                                        <th>적용개수</th>
                                        <th>게시물수</th>
                                        <th>서비스</th>
                                        <th>상태</th>
                                        <!--<th>생성 시간</th>
                                        <th>업데이트 시간</th>-->
                                        <th>기능</th>
                                    </tr>
                                    </thead>

                                    <tbody>
                                    {{#subscriptionList}}
                                    <tr>
                                        <td>{{subsid}}</td>
                                        <td class="link">{{username}}</td>
                                        <td class="nowrap">{{min}}-{{max}}</td>
                                        <td class="nowrap">
                                            {{{use_posts_tag}}} / {{posts}}
                                        </td>
                                        <td>{{serviceList.name}}</td>
                                        <td>{{status}}</td>
                                        <!--<td><span class="nowrap">{{!createdate}}</span></td>
                                        <td><span class="nowrap">{{!updatedate}}</span></td>-->
                                        <td>{{{functag}}}</td>
                                    </tr>
                                    {{/subscriptionList}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="card-footer">
                            <ul class="pagination">
                                {{^subscriptionList.first}}
                                    <li>
                                        <a href="/customer/subscriptions/all/1" aria-label="First">
                                            <span aria-hidden="true"><i class="fas fa-angle-double-left"></i></span>
                                        </a>
                                    </li>
                                {{/subscriptionList.first}}
                                {{#pagelist}}
                                    <li {{#curpage}}class="active"{{/curpage}} ><a href="/customer/subscriptions/all/{{page}}">{{page}}</a></li>
                                {{/pagelist}}
                                {{^subscriptionList.last}}
                                    <li>
                                        <a href="/customer/subscriptions/all/{{subscriptionList.totalPages}}" aria-label="Last">
                                            <span aria-hidden="true"><i class="fas fa-angle-double-right"></i></span>
                                        </a>
                                    </li>
                                {{/subscriptionList.last}}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <!-- Main End -->
    </div>
</div>
</body>